{
  "build": {
    "beforeBuildCommand": "pnpm run build:web",
    "beforeDevCommand": "pnpm run dev:web",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Password Generator Pro",
    "version": "../package.json"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "all": true
      },
      "clipboard": {
        "all": true,
        "writeText": true,
        "readText": true
      },
      "window": {
        "setIcon": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Utility",
      "copyright": "Â© Password Generator Pro 2022. All rights reserved.",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/32x32.png",
        "icons/Square107x107.png",
        "icons/Square142x142.png",
        "icons/Square150x150.png",
        "icons/Square284x284.png",
        "icons/Square30x30.png",
        "icons/Square310x310.png",
        "icons/Square44x44.png",
        "icons/Square71x71.png",
        "icons/Square89x89.png"
      ],
      "identifier": "com.sebastienrousseau.password-generator-pro",
      "longDescription": "A fast, simple and powerful open-source cross platform utility tool for generating strong, unique and random passwords",
      "macOS": {
        "entitlements": "entitlements.plist",
        "exceptionDomain": "",
        "frameworks": [],
        "license": "../COPYRIGHT",
        "minimumSystemVersion": "10.13.6",
        "providerShortName": null,
        "signingIdentity": null
      },
      "publisher": "",
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    },
    "systemTray": {
      "iconAsTemplate": true,
      "iconPath": "icons/icon.png",
      "menuOnLeftClick": true
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "center": true,
        "fullscreen": false,
        "height": 600,
        "minHeight": 600,
        "minWidth": 600,
        "resizable": true,
        "title": "Password Generator Pro",
        "width": 600
      }
    ]
  }
}