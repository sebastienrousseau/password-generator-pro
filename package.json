{
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.3.1",
    "@tauri-apps/api": "^1.2.0",
    "@tauri-apps/cli": "^1.2.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prettier-plugin-rust": "^0.1.8",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "name": "password-generator",
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "build:web": "vite build",
    "build": "tauri build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "coverage:rust": "cargo tarpaulin --root=src-tauri --out Xml",
    "coverage:ts": "vitest run --dom --coverage",
    "dev:web": "vite",
    "dev": "tauri dev",
    "lint:check": "pnpm lint:rust:check && pnpm lint:ts:check",
    "lint:rust:check": "cd src-tauri && cargo fmt --check && cargo clippy",
    "lint:rust": "cd src-tauri && cargo fmt && cargo clippy --fix --allow-dirty",
    "lint:ts:check": "prettier --check . && yarn eslint webapp/ && yarn tsc --noemit",
    "lint:ts": "prettier --write . && yarn eslint webapp/ --fix && yarn tsc --noemit",
    "lint": "pnpm run lint:ts && pnpm run lint:rust",
    "preview": "vite preview",
    "tauri": "tauri",
    "test:rust": "cd src-tauri && cargo test",
    "test:ts": "vitest run --dom",
    "test": "pnpm run test:rust && pnpm run test:ts"
  },
  "type": "module",
  "version": "0.0.1"
}